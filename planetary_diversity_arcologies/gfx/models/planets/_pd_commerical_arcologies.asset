    name = "pd_commerical_city_planet_01_entity"
    locator = { name = "planetloc" position = { 0 0 0 } rotation = { 0 0 0 } }
    default_state = "idle"
    playback_rate = 1.0 # Animation speed 0
    state = { name = "idle" time_offset = { 0 100 }
