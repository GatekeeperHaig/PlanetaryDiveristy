#Default
entity = {
    name = "pd_capital_city_planet_01_entity"
    locator = { name = "planetloc" position = { 0 0 0 } rotation = { 0 0 0 } }
    default_state = "idle"
    playback_rate = 1.0 # Animation speed 0
    state = { name = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet_flyby" } }
	}
    attach = { "planetloc" = "pd_capital_city_planet_01_mesh" }
	attach = { "planetloc" = "pd_city_planet_cloud_mesh" }
	game_data = {
		shader_type = ship
		emissive_recolor_crunch = 5.0
	}
    scale = 1.0
}
entity = {
	name = "pd_capital_city_planet_01_mesh"
	pdxmesh = "planet_non_clouded_mesh"

	#planet surface texture override
	meshsettings = {
		name = "planet_geosphereShape"
		texture_diffuse = "pd_capital_city_planet_arthropoid_01_diffuse.dds"
		texture_normal = "pd_capital_city_planet_arthropoid_01_normal.dds"
		texture_specular = "pd_capital_city_planet_arthropoid_01_specular.dds"
		shader = "PdxMeshTerra"
	}
	##pole texture override
	meshsettings = {
		name = "polesShape"
		texture_diffuse = "poles_tropical_1_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
		shader = "AlphaBlend_00"
	}
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet_flyby" } }
	}
	attach = { planet = "city_planet_surface_ships_entity" }
	game_data = {
		shader_type = ship
		emissive_recolor_crunch = 5.0
	}
}
