
entity = {
	name = "pd_aw_commerical_planet_billboard_mesh"
	pdxmesh = "aw_planet_cloud_mesh"
	scale = 1.015
	##cloud texture override
	meshsettings = {
		name = "clouds_geosphereShape"
		texture_diffuse = "cloud_aw_commerical_billboards_diffuse.dds"
		texture_normal = "cloud_aw_commerical_billboards_normal.dds"
		texture_specular = "cloud_aw_commerical_billboards_spec.dds"
		shader = "PdxMeshAlphaAdditive"
	}
	##cloud shadow texture override
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "noalpha.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
		shader = "PdxMeshAlphaAdditive"
	}
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		animation_speed = 0.50
	}
}
